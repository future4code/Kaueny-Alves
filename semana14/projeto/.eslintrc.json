{
    "env": {
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb-base",
  "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier"
        
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
